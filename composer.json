{
    "name": "webbongga/wordpress-2",
    "description": "Wordpress project using Composer",
    "authors": [{
        "name": "Randolf Sartagoda",
        "email": "randolf.sartagoda@gmail.com"
    }],
    "require": {
        "php": ">=5.6",
        "johnpbloch/wordpress": "^4.8",
        "wpackagist-plugin/autoptimize": "*",
        "wpackagist-plugin/bwp-recaptcha": "*",
        "wpackagist-plugin/contact-form-7": "*",
        "wpackagist-plugin/contact-form-fcwdb7": "*",
        "wpackagist-plugin/easy-twitter-feed-widget": "*",
        "wpackagist-plugin/instant-comment-validation": "*",
        "wpackagist-plugin/mailpoet": "*",
        "wpackagist-plugin/optimus": "*",
        "wpackagist-plugin/simple-301-redirects": "*",
        "wpackagist-plugin/user-role-editor": "*",
        "wpackagist-plugin/wck-custom-fields-and-custom-post-types-creator": "*",
        "wpackagist-plugin/wordfence": "*",
        "wpackagist-plugin/wordpress-seo": "*",
        "webbongga/template-basic": "dev-master"
    },
    "repositories": [{
        "type": "composer",
        "url": "https://wpackagist.org"
    }, {
        "type": "git",
        "url": "git@gitlab.clickablebrand.com:webbongga/template-basic.git"
    }],
    "extra": {
        "installer-paths": {
            "content/plugins/{$name}/": ["type:wordpress-plugin"],
            "content/themes/{$name}/": ["type:wordpress-theme"]
        },
        "wordpress-install-dir": "core"
    }
}